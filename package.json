{
  "name": "infix-rpn-eval",
  "version": "1.0.0",
  "engineStrict": true,
  "engines": {
    "node": ">=16 <17"
  },
  "description": "Convert math expressions between postfix (RPN) and infix notations and evaluate them",
  "keywords": [
    "infix",
    "postfix",
    "dijkstra",
    "shunting",
    "yard",
    "calculator",
    "rpn"
  ],
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "prepare": "rm -rf .git/hooks && ln -s $(pwd)/git-hooks .git/hooks",
    "pretest": "eslint src --fix",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taxnuke/infix-rpn-eval.git"
  },
  "author": "Semyon Fomin <taxnuke@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/semyonf/infix-rpn-eval/issues"
  },
  "homepage": "https://github.com/semyonf/infix-rpn-eval#readme",
  "devDependencies": {
    "@tsconfig/node16-strictest": "1.0.1",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.27",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "eslint": "7.17.0",
    "jest": "27.4.3",
    "prettier": "2.6.2",
    "ts-jest": "27.0.7",
    "ts-node": "8.10.2",
    "typescript": "4.1.6"
  }
}
